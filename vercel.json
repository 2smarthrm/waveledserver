 
{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
 // vercel.json (no projeto do admin)
{
  "rewrites": [
    { "source": "/api/(.*)", "destination": "https://waveledserver.vercel.app/api/$1" }
  ]
}

}
